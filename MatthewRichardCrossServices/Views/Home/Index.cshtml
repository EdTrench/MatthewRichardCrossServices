@model IEnumerable<MatthewRichardCrossServices.Models.PortfolioSummaryItem>
@{
    ViewBag.Title = "Matthew Richard Cross Services";
}

<ul id="picture-carousel" class="mycarousel jcarousel-skin-ie7">
    @foreach(var item in Model)
    {
        <li>
            <a href="/" title = "@Html.DisplayFor(modelItem => item.Name)">
            <img src=@Html.DisplayFor(modelItem => item.PhotographLocation) width="100%" height="550" 
            alt="@Html.DisplayFor(modelItem => item.DescriptionText)" 
            class="mycaptify captify-skin-captify_text"/>
        </li>
    }
</ul>

